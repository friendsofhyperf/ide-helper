{
    "name": "friendsofhyperf/ide-helper",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "The ide-helper component for Hyperf.",
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\IdeHelper\\": "src/"
        }
    },
    "require": {
        "barryvdh/reflection-docblock": "^2.0",
        "composer/composer": "^1.0|^2.0",
        "hyperf/filesystem": "~2.2.0"
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "doctrine/dbal": "Required to rename columns (^2.5|^3.0)."
    },
    "extra": {
        "branch-alias": {
            "2.x-dev": "2.0-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\IdeHelper\\ConfigProvider"
        }
    }
}